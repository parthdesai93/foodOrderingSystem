<h1>Listing Menu Items</h1>

  <h3 class="text-center">Menu Items</h3>

  <div class="row">
    <div class="col-xs-6 col-xs-offset-3">
      <% @menu_items.each do |menu_item| %>

          <%= render "menu_item_row", menu_item: menu_item, order_item: @order_item %>
      <% end %>
    </div>
  </div>
